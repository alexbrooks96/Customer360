<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.delaware.ab.trac2018.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Customers Table">
				<content>
					<List id="customerList" items="{customersModel>/customers}">
						<ObjectListItem title= "{customersModel>name}">
				<attributes>
					<ObjectAttribute text="{customersModel>city}" />
					<ObjectAttribute text="{customersModel>countrykey}" />
				</attributes>
				
				</ObjectListItem>
						
						
					</List>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>